<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>net45;netcoreapp3.1</TargetFrameworks>
    <OutputType>WinExe</OutputType>
    <RootNamespace>SystemThemedForms</RootNamespace>
    <AssemblyName>System Themed Forms</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>Resources\Krypton.ico</ApplicationIcon>
  </PropertyGroup>
    <PropertyGroup>
        <OutputPath>..\..\..\Binaries\Krypton Toolkit Examples\$(Configuration)\</OutputPath>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Krypton.Toolkit" Version="5.500.2002.1" />
    </ItemGroup>
</Project>